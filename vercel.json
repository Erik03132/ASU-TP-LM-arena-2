{
  "version": 2,
  "name": "asu-tp-systems",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/zhkh",
      "destination": "/asu-tp-zhkh.html"
    },
    {
      "source": "/food",
      "destination": "/pischevaya-promyshlennost.html"
    },
    {
      "source": "/engineering",
      "destination": "/mashinostroenie.html"
    },
    {
      "source": "/oil-gas",
      "destination": "/neftegazservis.html"
    },
    {
      "source": "/logistics",
      "destination": "/logistika.html"
    }
  ]
}
